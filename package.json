{
  "name": "vite-plugin-hot-vfs",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.5.2",
  "description": "Vite plugin for hosting vfs with HMR",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vite-plugin-hot-vfs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vite-plugin-hot-vfs.git"
  },
  "bugs": "https://github.com/antfu/vite-plugin-hot-vfs/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "pathe": "^2.0.3",
    "undio": "^0.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.4.0",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^9.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.9",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.4.3",
    "simple-git-hooks": "^2.11.1",
    "tinyexec": "^0.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "unstorage": "^1.15.0",
    "vite": "^6.2.0",
    "vitest": "^3.0.7",
    "vitest-package-exports": "^0.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
